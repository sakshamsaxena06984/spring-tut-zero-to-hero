package com.springcore.springcore.autowire;

public class Parent {
	Stu stu;

	public Parent() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Parent(Stu stu) {
		super();
		this.stu = stu;
	}

	public Stu getStu() {
		return stu;
	}

	public void setStu(Stu stu) {
		this.stu = stu;
	}

	@Override
	public String toString() {
		return "Parent [stu=" + stu + "]";
	}
	

}
